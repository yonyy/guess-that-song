{
  "name": "guess-that-song",
  "version": "1.0.0",
  "description": "Node app that plays music and users have to guess the track and artist(s)",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "bundle": "browserify -v -t [ babelify loose-envify ] src/react/index.js | uglifyjs -m -c warnings=false -o dist/bundle.js",
    "watch": "watchify -v -t [ babelify loose-envify ] src/react/index.js -o dist/bundle.js",
    "compress": "gzip -f < dist/bundle.js > dist/bundle.js.gz",
    "build": "yarn run bundle && yarn run compress",
    "server": "nodemon -V app.js",
    "dev": "yarn run watch & yarn run server"
  },
  "author": "Yonatan Perez",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "base-64": "^0.1.0",
    "bcrypt": "^1.0.2",
    "connect-gzip-static": "^2.0.1",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.4",
    "graphql-server-express": "^1.0.0",
    "graphql-tag": "^2.4.2",
    "grommet": "^1.5.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.3",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "randomstring": "^1.1.5",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "request": "^2.81.0",
    "spotify-web-api-js": "^0.22.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "bundle-collapser": "^1.2.1",
    "envify": "^4.1.0",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "uglify-js": "^3.0.25",
    "uglifyify": "^4.0.2"
  }
}
